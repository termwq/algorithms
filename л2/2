using System;

// Из 10-ти введённых чисел вывести количество оканчивающихся на '3' значений

class three {
    public static void Main() {
        int count=0;
        for (int i=0; i<10; i++) {
            string n = Console.ReadLine();
            if (n[n.Length-1] == '3') {
                ++count;
            }
        }
        Console.WriteLine("Количество: {0}", count);
    }
}
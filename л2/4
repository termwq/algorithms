using System;

// Из 10-ти введённых чисел вывести произведение чётных неотрицательных

class Multiplication
{
    public static void Main()
    {
        int m = 1;
        for (int i = 0; i < 10; i++)
        {
            int n=Convert.ToInt32(Console.ReadLine());
            if (n >= 0 && n%2==0 )
            {
                m = m * n;

            }

        }
        Console.WriteLine($"Произведение: {m}");
        }
    }